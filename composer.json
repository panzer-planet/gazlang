{
    "name": "werner/gazlang",
    "description": "A simple programming language compiler",
    "type": "project",
    "require": {
        "php": ">=7.4",
      "ext-ctype": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "laravel/pint": "^1.21"
    },
    "autoload": {
        "psr-4": {
            "GazLang\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GazLang\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
